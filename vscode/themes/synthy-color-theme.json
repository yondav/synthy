		//terminal
		"terminal.background": "#161618",
		"terminal.foreground": "#f1ebf3",
		"terminal.ansiBlack": "#222",
		"terminal.ansiBrightBlack": "#333",
		"terminal.ansiBrightWhite": "#ffffff",
		"terminal.ansiWhite": "#fafafa",
		"terminal.ansiRed": "#ff8d8d",
